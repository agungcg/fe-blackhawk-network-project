<script type='text/javascript'>

    console.log("data dashboar", <%- JSON.stringify(dashboard) %>)

    Chart.defaults.global.defaultFontFamily = '-apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif';
    Chart.defaults.global.defaultFontColor = '#292b2c';

    var chartVisitCompliance = document.getElementById("chartVisitCompliance");
    var chartFixtureCompliance = document.getElementById("chartFixtureCompliance");
    var chartPOGCompliance = document.getElementById("chartPOGCompliance");
    var chartPODCompliance = document.getElementById("chartPODCompliance");
    var chartPOPCompliance = document.getElementById("chartPOPCompliance");
    var chartInStockCompliance = document.getElementById("chartInStockCompliance");
    var chartActivationKnowHow = document.getElementById("chartActivationKnowHow");
    var chartPromotionAwareness = document.getElementById("chartPromotionAwareness");
    var chartComplaintHandling = document.getElementById("chartComplaintHandling");

    
    var tempDashboardVisitCompliancePersen = <%- dashboard.visitCompliance.persen %>
    var tempDashboardFixtureComplianceEntry = <%- dashboard.fixtureCompliance.entry %>
    var tempDashboardFixtureComplianceExit = <%- dashboard.fixtureCompliance.exit %>
    var tempDashboardPOGComplianceEntry = <%- dashboard.POGCompliance.entry %>
    var tempDashboardPOGComplianceExit = <%- dashboard.POGCompliance.exit %>
    var tempDashboardPODComplianceEntry = <%- dashboard.PODCompliance.entry %>
    var tempDashboardPODComplianceExit = <%- dashboard.PODCompliance.exit %>
    var tempDashboardPOPComplianceEntry = <%- dashboard.POPCompliance.entry %>
    var tempDashboardPOPComplianceExit = <%- dashboard.POPCompliance.exit %>
    var tempDashboardInstockComplianceEntryInstockCompliance = <%- dashboard.instockCompliance.entryInstockCompliance %>
    var tempDashboardInstockComplianceExitInstockCompliance = <%- dashboard.instockCompliance.exitInstockCompliance %>
    var tempDashboardActivationKnowHowPersen = <%- dashboard.activationKnowHow.persen %>
    var tempDashboardPromotionAwarenessPersen = <%- dashboard.promotionAwareness.persen %>
    var tempDashboardComplaintHandlingPersen = <%- dashboard.complaintHandling.persen %>

    var tempvisitCompliancePersen = 0
    if (tempDashboardVisitCompliancePersen != null) {
        if (tempDashboardVisitCompliancePersen <= 100) {
            tempvisitCompliancePersen = 100 - tempDashboardVisitCompliancePersen
        }
    }

    var tempactivationKnowHowPersen = 0
    if (tempDashboardActivationKnowHowPersen != null) {
        if (tempDashboardActivationKnowHowPersen <= 100) {
            tempactivationKnowHowPersen = 100 - tempDashboardActivationKnowHowPersen
        }
    }

    var temppromotionAwarenessPersen = 0
    if (tempDashboardPromotionAwarenessPersen != null) {
        if (tempDashboardPromotionAwarenessPersen <= 100) {
            temppromotionAwarenessPersen = 100 - tempDashboardPromotionAwarenessPersen
        }
    }
    
    var tempcomplaintHandlingPersen = 0
    if (tempDashboardComplaintHandlingPersen != null) {
        if (tempDashboardComplaintHandlingPersen <= 100) {
            tempcomplaintHandlingPersen = 100 - tempDashboardComplaintHandlingPersen
        }
    }
    
    var configChartVisitCompliance = new Chart(chartVisitCompliance, {
        type: 'pie',
        data: {
            labels: ['Percent'],
            datasets: [{
                data: [tempDashboardVisitCompliancePersen, tempvisitCompliancePersen],
                backgroundColor: ['#007bff', '#f7f7f7'],
            }],
        },
    });

    var configChartFixtureCompliance = new Chart(chartFixtureCompliance, {
        type: 'pie',
        data: {
            labels: ['Entry', 'Exit'],
            datasets: [{
            data: [tempDashboardFixtureComplianceEntry, tempDashboardFixtureComplianceExit, 200 - (tempDashboardFixtureComplianceEntry + tempDashboardFixtureComplianceExit)],
            backgroundColor: ['#007bff', '#00cec9', '#f7f7f7'],
            }],
        },
    });

    var configChartPOGCompliance = new Chart(chartPOGCompliance, {
        type: 'pie',
        data: {
            labels: ['Entry', 'Exit'],
            datasets: [{
            data: [tempDashboardPOGComplianceEntry, tempDashboardPOGComplianceExit, 200 - (tempDashboardPOGComplianceEntry + tempDashboardPOGComplianceExit)],
            backgroundColor: ['#007bff', '#00cec9','#f7f7f7'],
            }],
        },
    });

    var configChartPODCompliance = new Chart(chartPODCompliance, {
        type: 'pie',
        data: {
            labels: ['Entry', 'Exit'],
            datasets: [{
                data: [tempDashboardPODComplianceEntry, tempDashboardPODComplianceExit, 200 - (tempDashboardPODComplianceEntry + tempDashboardPODComplianceExit)],
                backgroundColor: ['#007bff', '#00cec9', '#f7f7f7'],
            }],
        },
    });

    var configConfigChartPOPCompliance = new Chart(chartPOPCompliance, {
        type: 'pie',
        data: {
            labels: ['Entry', 'Exit'],
            datasets: [{
                data: [tempDashboardPOPComplianceEntry, tempDashboardPOPComplianceExit, 200 - (tempDashboardPOPComplianceEntry + tempDashboardPOPComplianceExit)],
                backgroundColor: ['#007bff', '#00cec9', '#f7f7f7'],
            }],
        },
    });

    var configChartInStockCompliance = new Chart(chartInStockCompliance, {
        type: 'pie',
        data: {
            labels: ['Entry', 'Exit'],
            datasets: [{
                data: [tempDashboardInstockComplianceEntryInstockCompliance, tempDashboardInstockComplianceExitInstockCompliance, 200 - (tempDashboardInstockComplianceEntryInstockCompliance + tempDashboardInstockComplianceExitInstockCompliance)],
                backgroundColor: ['#007bff', '#00cec9', '#f7f7f7'],
            }],
        },
    });

    var configChartActivationKnowHow = new Chart(chartActivationKnowHow, {
        type: 'pie',
        data: {
            labels: ['Percent'],
            datasets: [{
                data: [tempDashboardActivationKnowHowPersen, tempactivationKnowHowPersen],
                backgroundColor: ['#007bff', '#f7f7f7'],
            }],
        },
    });

    var configChartPromotionAwareness = new Chart(chartPromotionAwareness, {
        type: 'pie',
        data: {
            labels: ['Percent'],
            datasets: [{
                data: [tempDashboardPromotionAwarenessPersen, temppromotionAwarenessPersen],
                backgroundColor: ['#007bff', '#f7f7f7'],
            }],
        },
    });

    var configChartComplaintHandling = new Chart(chartComplaintHandling, {
        type: 'pie',
        data: {
            labels: ['Percent'],
            datasets: [{
                data: [tempDashboardComplaintHandlingPersen, tempcomplaintHandlingPersen],
                backgroundColor: ['#007bff', '#f7f7f7'],
            }],
        },
    });
</script>

