<script type='text/javascript'>
    Chart.defaults.global.defaultFontFamily = '-apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif';
    Chart.defaults.global.defaultFontColor = '#292b2c';
    
    var chartVisitCompliance = document.getElementById("chartVisitCompliance");
    var chartFixtureCompliance = document.getElementById("chartFixtureCompliance");
    var chartPOGCompliance = document.getElementById("chartPOGCompliance");
    var chartPODCompliance = document.getElementById("chartPODCompliance");
    var chartPOPCompliance = document.getElementById("chartPOPCompliance");
    var chartInStockCompliance = document.getElementById("chartInStockCompliance");
    var chartActivationKnowHow = document.getElementById("chartActivationKnowHow");
    var chartPromotionAwareness = document.getElementById("chartPromotionAwareness");
    var chartComplaintHandling = document.getElementById("chartComplaintHandling");

    if ('<%- trigger %>' === 'visit-compliance') {
        var configChartVisitCompliance = new Chart(chartVisitCompliance, {
            type: 'bar',
            data: {
                labels: ['Percent'],
                datasets: [{
                    label: null,
                    data: ['<%- Math.round(detail.persen) %>'],
                    backgroundColor: ['#007bff'],
                }],
            },
            options: {
                scales: {
                    yAxes: [{
                        ticks: {
                            beginAtZero: true,
                            max: 100,
                            maxTicksLimit: 20,
                        }
                    }]
                },
                legend :{
                    display : false
                }
            }
        });
        $(document).ready(function() {
            $('#dataTableVisitCompliance').DataTable({
                data: JSON.parse('<%- JSON.stringify(detail.allDataVisit) %>'),
                columns: [
                    { data: 'visit_date',
                        render : function (data, type, row) {
                            return formatDate(data)
                        }
                    },
                    { data: 'tbl_store.tbl_retailer.retailer_name' },
                    { data: 'tbl_store.store_code' },
                    { data: 'tbl_store.tbl_dc.DC_name' },
                ],
                columnDefs: [
                    {
                        targets: [0, 1, 2, 3],
                        className: 'text-center'
                    },
                    {   width: '25%', 
                        targets: [0, 1, 2, 3]
                    },
                ], 
                order: [[0, 'asc']],
            });
        });
    } else if ('<%- trigger %>' === 'fixture-compliance') {
        var configChartFixtureCompliance = new Chart(chartFixtureCompliance, {
            type: 'bar',
            data: {
                labels: ['Entry', 'Exit'],
                datasets: [{
                    data: ['<%- Math.round(detail.entry) %>', '<%- Math.round(detail.exit) %>'],
                    backgroundColor: ['#007bff', '#00cec9'],
                }],
            },
            options: {
                scales: {
                    yAxes: [{
                        ticks: {
                            beginAtZero: true,
                            max: 100,
                            maxTicksLimit: 20,
                        }
                    }]
                },
                legend :{
                    display : false
                }
            }
        });
        $(document).ready(function() {
            var table = $('#dataTableFixtureCompliance').DataTable({
                data: JSON.parse('<%- JSON.stringify(detail.dataFixComp) %>'),
                columns: [
                    { data: 'visit_date',
                        render : function (data, type, row) {
                            return formatDate(data)
                        }
                    },
                    { data: 'tbl_store.tbl_retailer.retailer_name' },
                    { data: 'tbl_store.store_code' },
                    { data: 'tbl_store.fixtureType1.fixture_type' },
                    { data: null,
                        render : function (data, type, row) {
                            if (row.entry_fixture_comp == true) {
                                return 'Yes'
                            } else {
                                if (row.entry_correct_fixture_id != null){
                                    return row.entry_correct_fixture_id.fixture_type
                                } else {
                                    return 'null'
                                }
                            }
                        }
                    },
                    { data: null,
                        render : function (data, type, row) {
                            if (row.exit_fixture_comp == true) {
                                return 'Yes'
                            } else {
                                if (row.exit_correct_fixture_id != null){
                                    return row.exit_correct_fixture_id.fixture_type
                                } else {
                                    return 'null'
                                }
                            }
                        }
                    },
                ],
                columnDefs: [
                    {
                        targets: [0, 1, 2, 3, 4, 5],
                        className: 'text-center'
                    },
                    {   width: '16.7%', 
                        targets: [0, 1, 2, 3, 4, 5]
                    },
                ],
                order: [[0, 'asc']],
            });
        });
    } else if ('<%- trigger %>' === 'POG-compliance') {
        var configChartPOGCompliance = new Chart(chartPOGCompliance, {
            type: 'bar',
            data: {
                labels: ['Entry', 'Exit'],
                datasets: [{
                    data: ['<%- Math.round(detail.entry) %>', '<%- Math.round(detail.exit) %>'],
                    backgroundColor: ['#007bff', '#00cec9'],
                }],
            },
            options: {
                scales: {
                    yAxes: [{
                        ticks: {
                            beginAtZero: true,
                            max: 100,
                            maxTicksLimit: 20,
                        }
                    }]
                },
                legend :{
                    display : false
                }
